@code {
    [Parameter]
    public Dictionary<String, String> ValidRoutes { get; set; } = new Dictionary<String, String> {
{ "", "" },
};

    [Parameter]
    public string? bsclasses { get; set; } = String.Empty;

}

@foreach (KeyValuePair<string, string> route in ValidRoutes)
{
    <NavLink href="@route.Key" class="nav-link text-light text-nowrap fs-5">
    @route.Value
</NavLink>
}
